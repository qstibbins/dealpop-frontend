{
  "name": "dealpop-frontend-styled",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:prod": "tsc && vite build --mode production",
    "build:beta": "tsc && vite build --mode production",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "deploy:landing": "cp index.html dist/ && aws s3 sync dist/ s3://your-bucket-name --delete",
    "deploy:beta": "npm run build:beta && cp beta-index.html dist/index.html && aws s3 sync dist/ s3://your-bucket-name/beta/ --delete",
    "deploy:s3": "npm run build:prod && aws s3 sync dist/ s3://your-bucket-name --delete"
  },
  "dependencies": {
    "firebase": "^12.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.6.3"
  },
  "devDependencies": {
    "@types/chrome": "^0.1.24",
    "@types/html2canvas": "^1.0.0",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
